Infiniteyield.Size = UDim2.new(0, 104, 0, 40)
Infiniteyield.Font = Enum.Font.Sarpanch
Infiniteyield.Text = "Infinite yield"
Infiniteyield.TextColor3 = Color3.fromRGB(255, 255, 255)
Infiniteyield.TextSize = 19.000
