		local notLoaded = Instance.new("Message", game:GetService("CoreGui"))
		notLoaded.Text = 'Infinite Yield is waiting for the game to load'
		game.Loaded:Wait()
		notLoaded:Destroy()
	end

	ver = '5.4.1'

